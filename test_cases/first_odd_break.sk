// return first odd
// missing break statement
int first_odd(int[4] a){    
    int num = -1;
    
    for(int i=0; i<4; i++)
    {
        if ((+|-)a[i]%2==1)
        {
            num = a[i];
        }
    }
  
    return num;
}

harness void main(int x){
    int[4] a = {2, 3, 4, 5};
    assert first_odd(a) == a[1];
}
